{
  "name": "server",
  "version": "1.0.0",
  "description": "Server side of the full stack js example",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"tsc -w\" \"npx tsx src/index.ts\"",
    "start": "NODE_ENV=prod && node src/index.js",
    "db-setup": "docker-compose up -d && npx tsx test/seed-data.ts",
    "test": "vitest",
    "build": "rm -rf dist/ && tsc && cp .env dist/.env && cp src/schema.graphql dist/src/schema.graphql && cp src/vercel.json dist/src/vercel.json && cp package.json dist/package.json && cp package-lock.json dist/package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/king-koda/full-stack-js-example-18072024.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/king-koda/full-stack-js-example-18072024/issues"
  },
  "homepage": "https://github.com/king-koda/full-stack-js-example-18072024#readme",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@faker-js/faker": "^8.4.1",
    "@graphql-tools/schema": "^10.0.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.16.2",
    "typeorm": "^0.3.20",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/ws": "^8.5.11",
    "concurrently": "^8.2.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
}
